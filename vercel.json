{
  "buildCommand": "turbo run build --filter=@omni/web",
  "outputDirectory": "apps/web/.next",
  "installCommand": "bun install",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/_mintlify/:path*",
      "destination": "https://omnilens-626cb878.mintlify.app/_mintlify/:path*"
    },
    {
      "source": "/api/request",
      "destination": "https://omnilens-626cb878.mintlify.app/_mintlify/api/request"
    },
    {
      "source": "/docs",
      "destination": "https://omnilens-626cb878.mintlify.app/docs"
    },
    {
      "source": "/docs/llms.txt",
      "destination": "https://omnilens-626cb878.mintlify.app/llms.txt"
    },
    {
      "source": "/docs/llms-full.txt",
      "destination": "https://omnilens-626cb878.mintlify.app/llms-full.txt"
    },
    {
      "source": "/docs/sitemap.xml",
      "destination": "https://omnilens-626cb878.mintlify.app/sitemap.xml"
    },
    {
      "source": "/docs/robots.txt",
      "destination": "https://omnilens-626cb878.mintlify.app/robots.txt"
    },
    {
      "source": "/docs/mcp",
      "destination": "https://omnilens-626cb878.mintlify.app/mcp"
    },
    {
      "source": "/docs/:path*",
      "destination": "https://omnilens-626cb878.mintlify.app/docs/:path*"
    },
    {
      "source": "/mintlify-assets/:path+",
      "destination": "https://omnilens-626cb878.mintlify.app/mintlify-assets/:path+"
    }
  ]
}